<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/pkg_bg"
    android:orientation="vertical"
    android:paddingTop="@dimen/_10sdp">

    <TextView
        android:id="@+id/my_packages_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/my_packages_ivBack"
        android:layout_toRightOf="@+id/my_packages_ivBack"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:paddingTop="@dimen/_8sdp"
        android:paddingBottom="@dimen/_8sdp"
        android:singleLine="true"
        android:text="Test"
        android:textColor="@color/white"
        android:textSize="@dimen/regular_text_size"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/my_packages_ivBack"
        android:layout_width="@dimen/_35sdp"
        android:layout_height="@dimen/_35sdp"
        android:padding="@dimen/_10sdp"
        android:src="@drawable/back_white_icon"
        android:visibility="gone"
        tools:ignore="ContentDescription" />

    <androidx.core.widget.NestedScrollView
        android:id="@+id/my_packages_scroll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/my_packages_header"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:id="@+id/card_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_10sdp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/_5sdp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="@dimen/_10sdp">

                    <TextView
                        android:id="@+id/my_packages_tvReportHeader"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:singleLine="true"
                        android:text="My Performance"
                        app:layout_constraintTop_toTopOf="parent"
                        android:textColor="@color/header_gray"
                        android:textSize="@dimen/normal_header_size" />

                    <com.ramijemli.percentagechartview.PercentageChartView
                        android:id="@+id/my_packages_ivProgress"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_130sdp"
                        android:layout_marginTop="@dimen/_20sdp"
                        app:layout_constraintTop_toBottomOf="@+id/my_packages_tvReportHeader"
                        app:pcv_animDuration="800"
                        app:pcv_animInterpolator="anticipate_overshoot"
                        app:pcv_backgroundBarColor="@color/default_shimmer_color"
                        app:pcv_backgroundBarThickness="@dimen/_10sdp"
                        app:pcv_mode="ring"
                        app:pcv_orientation="clockwise"
                        app:pcv_progressBarThickness="@dimen/_10sdp"
                        app:pcv_progressColor="@color/green"
                        app:pcv_startAngle="270"
                        app:pcv_textColor="@color/white"
                        app:pcv_textSize="@dimen/_35sdp"
                        app:pcv_textStyle="bold" />

                    <TextView
                        android:id="@+id/my_packages_tvProgress"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:background="@color/white"
                        android:gravity="center"
                        android:padding="@dimen/_10sdp"
                        android:textSize="@dimen/_30sdp"
                        app:layout_constraintBottom_toBottomOf="@+id/my_packages_ivProgress"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/my_packages_ivProgress" />

                    <ImageView
                        android:id="@+id/my_packages_ivReport"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:src="@drawable/diagram"
                        android:layout_marginLeft="@dimen/_10sdp"
                        app:layout_constraintBottom_toBottomOf="@+id/my_packages_tvReport"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/my_packages_tvReport"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:id="@+id/my_packages_tvReport"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:text="Knowledge\nGap"
                        android:textColor="@color/black"
                        android:textSize="@dimen/normal_text_size"
                        app:layout_constraintLeft_toRightOf="@+id/my_packages_ivReport"
                        app:layout_constraintRight_toLeftOf="@+id/my_packages_ivPendingTest"
                        app:layout_constraintTop_toBottomOf="@+id/my_packages_ivProgress" />

